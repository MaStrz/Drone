import cv2
import numpy as np

fname = './single/HD_18_im1.jpg'
img = cv2.imread(fname)
      

with np.load('./camera_coeffs.npz') as X:
	mtx, dist, _, _ = [X[i] for i in ('arr_0','arr_1','arr_2','arr_3')]















dst = cv2.undistort(img, mtx, dist, None)

# crop the image
x,y,w,h = roi
dst = dst[y:y+h, x:x+w]
cv2.imwrite('calibresult.png',dst)

